.myScroll {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: white;
}

.myCell {
  margin-top: 10px;
  height: 100px;
  background-color: lightgray;
  border-radius: 5px;
  border: 1rpx solid #e0e0e0;
  flex-direction: row;
  align-items: center;
  display: flex;
}

.myImage {
  width: 88rpx;
  height: 88rpx;
  border-radius: 44rpx;
  margin-left: 10px;
  background-color: #eee;
}

.myRightView {
  margin-left: 10px;
  margin-top: 10px;
  flex-direction: column;
  display: flex;
}

.myUserName {
  margin-top: 10px;
  font-size: 14px;
  color: black;
}

.myTel {
  margin-top: 10px;
  font-size: 12px;
  color: sandybrown;
}

.myAddress {
  margin-top: 10px;
  font-size: 12px;
  color: black;
}
